<% if(page_num>0){%>
  <nav aria-label="...">
      <ul class="pagination small">
        
        <li class="page-item <%= start===1 ? 'disabled' : '' %>">
          <a class="page-link" href="<%= '/admin/'+ api_name +'?dataName=' + dataName + '&rule=' + rule + '&q=' + q + '&start=' + (start-1) + '&num=' + num %>">上一页</a>
        </li>
        
        <% for(var i=1; i<=page_num; i++) {%>
        <li class="page-item <%= start===i ? 'active' : '' %>">
          <a class="page-link" href="<%= '/admin/'+ api_name +'?dataName=' + dataName + '&rule=' + rule + '&q=' + q + '&start=' + i + '&num=' + num %>"><%= i %></a>
        </li>
        <% }%>
  
        <li class="page-item <%= start==page_num ? 'disabled' : '' %>">
          <a class="page-link" href="<%= '/admin/'+ api_name +'?dataName=' + dataName + '&rule=' + rule + '&q=' + q + '&start=' + (start+1) + '&num=' + num %>">下一页</a>
        </li>
        
      </ul>
    </nav>
<% }%>